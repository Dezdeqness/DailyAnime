    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="Дата выхода" />

</androidx.appcompat.widget.LinearLayoutCompat>
